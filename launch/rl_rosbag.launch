<launch>

  <param name="/use_sim_time" value="true" />

  <param name="/odom_in_tf"  value="false" />

  <node pkg="virtual_imu" type="virtual_imu" name="virtual_imu" />

  <node pkg="ackermann_to_odom" type="ackermann_to_odom" name="ackermann_to_odom" />

  <!-- execute the static tf of your robot -->
  <include file="$(find robot_blue)/launch/start_tf.launch" />

  <include file="$(find robot_localization)/launch/start_robot_localization.launch" />

  <!-- place the path of your bag file -->
  <node pkg="rosbag" type="play" name="rosbagplay" args="/home/mice85/aurova-lab/labrobotica/local/Datasets/AUROVA/2018-06-21\ experimentos/2018-06-21_1.bag --clock -d 5" required="true"/>

  <node type="rviz" name="rviz" pkg="rviz" args="-d $(find application_localization)/rviz/rl.rviz" />

</launch>
